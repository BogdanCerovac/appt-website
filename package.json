{
  "name": "appt",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build:dev": "env-cmd -f .env docusaurus build",
    "build:acc": "env-cmd -f .env.acc docusaurus build",
    "build:prod": "env-cmd -f .env.prod docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "lint": "eslint --ext  \".js,.ts,.tsx,.md,.mdx\" --ignore-path .eslintignore .",
    "lint:fix": "eslint --ext  \".js,.ts,.tsx,.md,.mdx\" --ignore-path .eslintignore . --fix",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "update-accessibility-data": "env-cmd -f .env.secrets tsx scripts/accessibility-data-update.ts"
  },
  "dependencies": {
    "@docusaurus/core": "3.2.1",
    "@docusaurus/preset-classic": "3.2.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "lodash": "^4.17.21",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-youtube": "^10.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.2.1",
    "@docusaurus/tsconfig": "3.2.1",
    "@docusaurus/types": "3.2.1",
    "@google-cloud/storage": "^7.10.0",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "autoprefixer": "^10.4.19",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-prettier": "^5.1.3",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.7.2",
    "typescript": "~5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
